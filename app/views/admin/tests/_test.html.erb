<tr>
  <td class="align-middle">
    <%= content_tag :span, test.title, class: 'test-title', data: { test_id: test.id } %> </div>
    <%= render 'form_inline', test: test == @test ? @test : test %>
  </td> 
  <td class="text-center align-middle"><%= " #{test_level(test)}" %></td>
  <td class="text-center align-middle"> <%= " #{test.questions.count}" %></td> 
  
  <td class="align-middle"><%= link_to t('nav.show'), admin_test_path(test) %></td> 
  <td class="align-middle">
    <%= link_to t('nav.edit'), edit_admin_test_path(test), data: { test_id: test.id }, class: 'form-inline-link' %>
  </td> 
  <td class="align-middle"><%= link_to t('nav.del'), admin_test_path(test), method: 'delete', data: { confirm: 'Are you sure?' } %></td> 
</tr>
